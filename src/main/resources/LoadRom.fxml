<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>


<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.hexrobot.fe5randomizer.controllers.LoadRomController">
   <children>
      <fx:include fx:id="menuBar" source="MenuBar.fxml" />
      <Button fx:id="btnLoadRom" mnemonicParsing="false" onAction="#openFileDialog" text="Load ROM...">
         <VBox.margin>
            <Insets bottom="20.0" left="50.0" right="50.0" top="20.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
