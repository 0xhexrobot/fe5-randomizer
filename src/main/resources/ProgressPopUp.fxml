<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>


<VBox fx:id="progressPopUp" spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.hexrobot.fe5randomizer.controllers.ProgressPopUpController">
   <children>
      <Label fx:id="lblMessage" text="Label" />
      <ProgressBar fx:id="progressBar" prefWidth="200.0" progress="0.0" />
      <HBox>
         <children>
            <Pane HBox.hgrow="ALWAYS" />
            <Button fx:id="btnCancel" mnemonicParsing="false" text="Cancel" HBox.hgrow="NEVER" />
         </children>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="30.0" right="30.0" top="20.0" />
   </padding>
</VBox>
